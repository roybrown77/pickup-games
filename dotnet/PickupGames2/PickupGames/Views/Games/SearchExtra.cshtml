@model PickupGames.Models.GameSearchModel

<form id="searchgamesextraform" role="form" class="form-horizontal">    
    @Html.ValidationSummary(false)
    <div class="form-group">
        @Html.LabelFor(m => m.Name, new { @class = "col-md-2 control-label" })
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.Name, new { @class = "form-control" })
        </div>
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Sport, new { @class = "col-md-2 control-label" })
                
        <div class="col-md-10">
            @Html.TextBoxFor(m => m.Sport, new { @class = "form-control" })
        </div>
    </div>
</form>
